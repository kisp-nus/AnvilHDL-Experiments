enum ciph_op_e{
    NOP1,
    CIPH_FWD,
    CIPH_INV,
    NOP2
}

enum key_len_e {
    NOP1,
    KEY_LEN_128,
    KEY_LEN_192,
    NOP2,
    KEY_LEN_256,
    NOP3
}

enum sp2v_e {
    NOP1,
    NOP2,
    NOP3,
    SP2V_HIGH,
    SP2V_LOW,
    NOP4
}

struct init_pack{
    op : ciph_op_e,
    key_len : key_len_e
}
struct aes_key_expand_in_pack {
    key : logic[8][32],
    round : logic[4],
    ctrl : init_pack
}

struct aes_key_expand_out_pack {
    key : logic[8][32],
    error : logic
}
