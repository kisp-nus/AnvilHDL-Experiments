struct sbox_in{
    data : logic[8],
    op : ciph_op_e
}
chan sbox_ch {
    left req : (sbox_in@#1) @#1 - @#1,
    right res : (logic[8]@#1) @#1 - @#1
}


func circ_byte_shift(data, shift){
    match shift {
        2'h0 => data,
        2'h1 => #{data[2], data[1], data[0], data[3]},
        2'h2 => #{data[1], data[0], data[3], data[2]},
        2'h3 => #{data[0], data[3], data[2], data[1]},
        _ => data
    }
}
func div2(data){
    #{data[7], data[6], data[5], data[4]^data[0], data[3]^data[0], data[2], data[1]^data[0], data[0]}
}


func transpose(data : logic[4][4][8]){
    let row_1_t = #{data[0][3], data[0][2], data[0][1], data[0][0]};
    let row_2_t = #{data[1][3], data[1][2], data[1][1], data[1][0]};
    let row_3_t = #{data[2][3], data[2][2], data[2][1], data[2][0]};
    let row_4_t = #{data[3][3], data[3][2], data[3][1], data[3][0]};
    #{row_4_t, row_3_t, row_2_t, row_1_t}
}

func mul2(data){
    #{data[6], data[5],data[4], data[3]^data[7], data[2]^data[7],data[1], data[0]^data[7], data[7]}
}

func mul4(data){
    let data_mult2 = call mul2(data);
    call mul2(data_mult2)
}


